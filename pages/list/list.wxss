/* pages/list.wxss */
@import '/pages/template/template.wxss';
@import '/style/wxss/seachBar.wxss';

.scroll{
  height: 100%;
}
.carousel{
  border-bottom-left-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
  background-color: #489B81;
 
}
.carousel .carousel1,.carousel2,.carousel3{
  margin:40rpx 20rpx;
  width: 95%;
  height: 300rpx;
  border-radius: 16rpx;
}
.container{
  column-count: 2;
  column-gap: 0;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
}
swiper{
  width: 100%;
  height: 400rpx; 
}
swiper image{
  width: 100%;
  height: 100%;
  border-radius: 16rpx;
}
/* 返回顶部 */
.upImg{
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  bottom: 50rpx;
  background: rgba(128, 123, 123, 0.3);
  right: 30rpx;
  border-radius: 50%;
}

